{
  "package": {
    "name": "node-exporter",
    "repo": "debian",
    "subject": "phakudicompany"
  },

  "version": {
    "name": "@PACKAGE_VERSION@-@PACKAGE_REVISION@",
    "desc": "Version @PACKAGE_VERSION@-@PACKAGE_REVISION@",
    "released": "@PACKAGE_DATE@",
    "vcs_tag": "@PACKAGE_MODULE@-v@PACKAGE_VERSION@-@PACKAGE_REVISION@"
  },

  "files":
  [
    {
      "includePattern": "@PACKAGE_MODULE@/@PACKAGE_TYPE@/dist/(.*\\.deb)", "uploadPattern": "/pool/n/node-exporter/$1",
      "matrixParams": {
        "deb_distribution": "bionic,xenial,jessie,stretch,trusty",
        "deb_component": "node-exporter",
        "deb_architecture": "amd64,armhf,arm64,armel,ppc64el,mips64el",
        "override": 1
      }
    }
  ],
  "publish": true
}
